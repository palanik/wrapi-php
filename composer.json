{
    "name": "palanik/wrapi",
    "description": "Wrapper for calling Restful API",
    "type": "library",
    "keywords": [
        "Restful",
        "API",
        "wrapper",
        "functions",
        "api",
        "function",
        "guzzle"
    ],
    "license": "MIT",
    "homepage": "https://github.com/palanik/wrapi-php",
    "authors": [
        {
            "name": "palanik"
        }
    ],
    "archive": {
        "exclude": [".git", ".gitignore", "travis.yml", "/tests", "phpunit.xml.dist"]
    },
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": ">=6.1.0",
        "sabre/uri": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "internations/http-mock": "0.7.0"
    },
    "autoload": {
        "psr-4": {
            "wrapi\\": "src/"
        }
    }
}
